{
    "manifest_version": 3,
    "name": "Fake News Highlighter",
    "version": "1.0",
    "description": "An extension to highlight fake news on web pages.",
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": [
      "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  